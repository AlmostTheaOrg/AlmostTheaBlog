@model List<TheaBlog.Models.Category>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@foreach (var item in Model)
{
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>

    foreach (var photo in item.Photos)
    {
        <dd>
            <img src="~/Files?id=@photo.Image.FileId" alt="image" />
        </dd>
    }
}
